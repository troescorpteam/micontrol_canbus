# Example environment configuration for micontrol_canbus
# Copy this file to .env and modify the values as needed

# Path to the DBC file (relative or absolute path)
DBC_FILE=epc.dbc

# CAN interface name (e.g., can0, vcan0, etc.)
CAN_INTERFACE=can0

# Redis server URL for storing message data
REDIS_URL=redis://localhost:6379
